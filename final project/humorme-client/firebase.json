{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/users/**",
        "destination": "/users/[id].html"
      },
      {
        "source": "/jokes/**",
        "destination": "/jokes/[id].html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
